    якщо символ_КД == символи::МАЛА_БУКВА_ю {
      перший_байт[0] = п8(192п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_ю) >> 6));
      другий_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_ю) & 63));
      вернути 2;
    }
    якщо символ_КД == символи::МАЛА_БУКВА_я {
      перший_байт[0] = п8(192п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_я) >> 6));
      другий_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_я) & 63));
      вернути 2;
    }
    якщо символ_КД == символи::ВЕЛИКА_СПЕЦ_БУКВА_Ґ {
      перший_байт[0] = п8(192п32 | (п32(відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ґ) >> 6));
      другий_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ґ) & 63));
      вернути 2;
    }
    якщо символ_КД == символи::ВЕЛИКА_СПЕЦ_БУКВА_Ё {
      перший_байт[0] = п8(192п32 | (п32(відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ё) >> 6));
      другий_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ё) & 63));
      вернути 2;
