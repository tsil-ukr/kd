      другий_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_з) & 63));
      вернути 2;
    }
    якщо символ_КД == символи::МАЛА_БУКВА_и {
      перший_байт[0] = п8(192п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_и) >> 6));
      другий_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::МАЛА_БУКВА_и) & 63));
      вернути 2;
