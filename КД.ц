      вихід[0] = символи::МАЛА_БУКВА_и;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_і {
      вихід[0] = символи::МАЛА_БУКВА_і;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ї {
      вихід[0] = символи::МАЛА_БУКВА_ї;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_й {
      вихід[0] = символи::МАЛА_БУКВА_й;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_к {
      вихід[0] = символи::МАЛА_БУКВА_к;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_л {
      вихід[0] = символи::МАЛА_БУКВА_л;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_м {
      вихід[0] = символи::МАЛА_БУКВА_м;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_н {
      вихід[0] = символи::МАЛА_БУКВА_н;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_о {
      вихід[0] = символи::МАЛА_БУКВА_о;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_п {
      вихід[0] = символи::МАЛА_БУКВА_п;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_р {
      вихід[0] = символи::МАЛА_БУКВА_р;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_с {
      вихід[0] = символи::МАЛА_БУКВА_с;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_т {
      вихід[0] = символи::МАЛА_БУКВА_т;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_у {
      вихід[0] = символи::МАЛА_БУКВА_у;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ф {
      вихід[0] = символи::МАЛА_БУКВА_ф;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_х {
      вихід[0] = символи::МАЛА_БУКВА_х;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ц {
      вихід[0] = символи::МАЛА_БУКВА_ц;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ч {
      вихід[0] = символи::МАЛА_БУКВА_ч;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ш {
      вихід[0] = символи::МАЛА_БУКВА_ш;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_щ {
      вихід[0] = символи::МАЛА_БУКВА_щ;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ь {
      вихід[0] = символи::МАЛА_БУКВА_ь;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_ю {
      вихід[0] = символи::МАЛА_БУКВА_ю;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_БУКВА_я {
      вихід[0] = символи::МАЛА_БУКВА_я;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ґ {
      вихід[0] = символи::ВЕЛИКА_СПЕЦ_БУКВА_Ґ;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ё {
      вихід[0] = символи::ВЕЛИКА_СПЕЦ_БУКВА_Ё;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ъ {
      вихід[0] = символи::ВЕЛИКА_СПЕЦ_БУКВА_Ъ;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Ы {
      вихід[0] = символи::ВЕЛИКА_СПЕЦ_БУКВА_Ы;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::ВЕЛИКА_СПЕЦ_БУКВА_Э {
      вихід[0] = символи::ВЕЛИКА_СПЕЦ_БУКВА_Э;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_СПЕЦ_БУКВА_ґ {
      вихід[0] = символи::МАЛА_СПЕЦ_БУКВА_ґ;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_СПЕЦ_БУКВА_ё {
      вихід[0] = символи::МАЛА_СПЕЦ_БУКВА_ё;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_СПЕЦ_БУКВА_ъ {
      вихід[0] = символи::МАЛА_СПЕЦ_БУКВА_ъ;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_СПЕЦ_БУКВА_ы {
      вихід[0] = символи::МАЛА_СПЕЦ_БУКВА_ы;
      вернути так;
    }
    якщо символ_32 == відповідні_символи_юнікоду::МАЛА_СПЕЦ_БУКВА_э {
      вихід[0] = символи::МАЛА_СПЕЦ_БУКВА_э;
      вернути так;
    }
    вернути ні;
  }

  місцева дія перекодувати_з_Ю8(вхід: адреса<п8>, розмір: позитивне, вихід: адреса<п8>): позитивне {
    змінна ціль розмір_виходу: позитивне = 0;
    змінна ціль п: позитивне = 0;
    поки п < розмір {
      ціль перший_байт = вхід[п];
      ціль розмір_символа = дізнатись_розмір_символа_юнікоду(перший_байт);
      якщо розмір_символа == 2 {
        п += 1;
        ціль другий_байт = вхід[п];
        ціль символ_Ю16 = п16((п16(перший_байт & 31) << 6) | п16(другий_байт & 63));
        ціль символ_КД: п8 = 0;
        ціль успіх_отримання_КД_з_Ю32 = отримати_символ_КД_з_Ю32(символ_Ю16 як п32, символ_КД::адреса);
        якщо успіх_отримання_КД_з_Ю32 == ні {
          вернути 0;
        }
        вихід[розмір_виходу] = символ_КД;
        розмір_виходу += 1;
      } інакше якщо розмір_символа == 3 {
        п += 1;
        ціль другий_байт = вхід[п];
        п += 1;
        ціль третій_байт = вхід[п];
        ціль символ_Ю32 = п32((п32(перший_байт & 15) << 12) | (п32(другий_байт & 63) << 6) | п32(третій_байт & 63));
        ціль символ_КД: п8 = 0;
        ціль успіх_отримання_КД_з_Ю32 = отримати_символ_КД_з_Ю32(символ_Ю32, символ_КД::адреса);
        якщо успіх_отримання_КД_з_Ю32 == ні {
          вернути 0;
        }
        вихід[розмір_виходу] = символ_КД;
        розмір_виходу += 1;
      } інакше {
        вернути 0;
      }
      п += 1;
    }
    вернути розмір_виходу;
  }

  місцева дія перекодувати_в_Ю8(символ_КД: п8, перший_байт: адреса<п8>, другий_байт: адреса<п8>, третій_байт: адреса<п8>): п8 {
    якщо символ_КД == символи::НІЩО {
      перший_байт[0] = відповідні_символи_юнікоду::НІЩО;
      вернути 1;
    }
    якщо символ_КД == символи::СПЕЦ_СИМВОЛ_1 {
      перший_байт[0] = відповідні_символи_юнікоду::СПЕЦ_СИМВОЛ_1;
      вернути 1;
    }
    якщо символ_КД == символи::СПЕЦ_СИМВОЛ_2 {
      перший_байт[0] = відповідні_символи_юнікоду::СПЕЦ_СИМВОЛ_2;
      вернути 1;
    }
    якщо символ_КД == символи::СПЕЦ_СИМВОЛ_3 {
      перший_байт[0] = відповідні_символи_юнікоду::СПЕЦ_СИМВОЛ_3;
      вернути 1;
    }
    якщо символ_КД == символи::СПЕЦ_СИМВОЛ_4 {
      перший_байт[0] = відповідні_символи_юнікоду::СПЕЦ_СИМВОЛ_4;
      вернути 1;
    }
    якщо символ_КД == символи::МІЖРЯД {
      перший_байт[0] = відповідні_символи_юнікоду::МІЖРЯД;
      вернути 1;
    }
    якщо символ_КД == символи::МІЖСЛОВО {
      перший_байт[0] = відповідні_символи_юнікоду::МІЖСЛОВО;
      вернути 1;
    }
    якщо символ_КД == символи::ЗНАК_ОКЛИКУ {
      перший_байт[0] = відповідні_символи_юнікоду::ЗНАК_ОКЛИКУ;
      вернути 1;
    }
    якщо символ_КД == символи::ДВОЛАПКА {
      перший_байт[0] = відповідні_символи_юнікоду::ДВОЛАПКА;
      вернути 1;
    }
    якщо символ_КД == символи::РЕШІТКА {
      перший_байт[0] = відповідні_символи_юнікоду::РЕШІТКА;
      вернути 1;
    }
    якщо символ_КД == символи::МАТЕРІЯ {
      перший_байт[0] = п8(224п32 | (п32(відповідні_символи_юнікоду::МАТЕРІЯ) >> 12));
      другий_байт[0] = п8(128п32 | ((п32(відповідні_символи_юнікоду::МАТЕРІЯ) >> 6) & 63));
      третій_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::МАТЕРІЯ) & 63));
      вернути 3;
    }
    якщо символ_КД == символи::ГРИВНЯ {
      перший_байт[0] = п8(224п32 | (п32(відповідні_символи_юнікоду::ГРИВНЯ) >> 12));
      другий_байт[0] = п8(128п32 | ((п32(відповідні_символи_юнікоду::ГРИВНЯ) >> 6) & 63));
      третій_байт[0] = п8(128п32 | (п32(відповідні_символи_юнікоду::ГРИВНЯ) & 63));
      вернути 3;
    }
    якщо символ_КД == символи::ВІДСОТОК {
      перший_байт[0] = відповідні_символи_юнікоду::ВІДСОТОК;
      вернути 1;
    }
    якщо символ_КД == символи::АМПЕРСАНТ {
      перший_байт[0] = відповідні_символи_юнікоду::АМПЕРСАНТ;
      вернути 1;
    }
    якщо символ_КД == символи::ОДНОЛАПКА {
      перший_байт[0] = відповідні_символи_юнікоду::ОДНОЛАПКА;
      вернути 1;
    }
    якщо символ_КД == символи::ВІДКРИТА_ДУГА {
      перший_байт[0] = відповідні_символи_юнікоду::ВІДКРИТА_ДУГА;
      вернути 1;
    }
    якщо символ_КД == символи::ЗАКРИТА_ДУГА {
      перший_байт[0] = відповідні_символи_юнікоду::ЗАКРИТА_ДУГА;
      вернути 1;
    }
    якщо символ_КД == символи::ЗІРОЧКА {
      перший_байт[0] = відповідні_символи_юнікоду::ЗІРОЧКА;
      вернути 1;
    }
    якщо символ_КД == символи::ПЛЮС {
      перший_байт[0] = відповідні_символи_юнікоду::ПЛЮС;
      вернути 1;
    }
    якщо символ_КД == символи::КОМА {
      перший_байт[0] = відповідні_символи_юнікоду::КОМА;
      вернути 1;
    }
    якщо символ_КД == символи::МІНУС {
      перший_байт[0] = відповідні_символи_юнікоду::МІНУС;
      вернути 1;
    }
    якщо символ_КД == символи::КРАПКА {
      перший_байт[0] = відповідні_символи_юнікоду::КРАПКА;
      вернути 1;
    }
    якщо символ_КД == символи::КОСА {
      перший_байт[0] = відповідні_символи_юнікоду::КОСА;
      вернути 1;
    }
    якщо символ_КД == символи::НУЛЬ {
      перший_байт[0] = відповідні_символи_юнікоду::НУЛЬ;
      вернути 1;
    }
    якщо символ_КД == символи::ОДИН {
      перший_байт[0] = відповідні_символи_юнікоду::ОДИН;
      вернути 1;
    }
    якщо символ_КД == символи::ДВА {
      перший_байт[0] = відповідні_символи_юнікоду::ДВА;
      вернути 1;
    }
    якщо символ_КД == символи::ТРИ {
      перший_байт[0] = відповідні_символи_юнікоду::ТРИ;
      вернути 1;
    }
    якщо символ_КД == символи::ЧОТИРИ {
      перший_байт[0] = відповідні_символи_юнікоду::ЧОТИРИ;
      вернути 1;
    }
    якщо символ_КД == символи::ПʼЯТЬ {
      перший_байт[0] = відповідні_символи_юнікоду::ПʼЯТЬ;
      вернути 1;
    }
    якщо символ_КД == символи::ШІСТЬ {
      перший_байт[0] = відповідні_символи_юнікоду::ШІСТЬ;
      вернути 1;
    }
    якщо символ_КД == символи::СІМ {
      перший_байт[0] = відповідні_символи_юнікоду::СІМ;
      вернути 1;
    }
    якщо символ_КД == символи::ВІСІМ {
      перший_байт[0] = відповідні_символи_юнікоду::ВІСІМ;
      вернути 1;
    }
    якщо символ_КД == символи::ДЕВʼЯТЬ {
      перший_байт[0] = відповідні_символи_юнікоду::ДЕВʼЯТЬ;
      вернути 1;
    }
    якщо символ_КД == символи::ДВОКРАПКА {
      перший_байт[0] = відповідні_символи_юнікоду::ДВОКРАПКА;
      вернути 1;
    }
    якщо символ_КД == символи::МЕНШЕ {
      перший_байт[0] = відповідні_символи_юнікоду::МЕНШЕ;
      вернути 1;
    }
    якщо символ_КД == символи::РІВНО {
      перший_байт[0] = відповідні_символи_юнікоду::РІВНО;
      вернути 1;
    }
    якщо символ_КД == символи::БІЛЬШЕ {
      перший_байт[0] = відповідні_символи_юнікоду::БІЛЬШЕ;
      вернути 1;
    }
    якщо символ_КД == символи::ЗНАК_ПИТАННЯ {
      перший_байт[0] = відповідні_символи_юнікоду::ЗНАК_ПИТАННЯ;
      вернути 1;
    }
    якщо символ_КД == символи::РАВЛИК {
      перший_байт[0] = відповідні_символи_юнікоду::РАВЛИК;
      вернути 1;
    }
    якщо символ_КД == символи::ВІДКРИТА_КВАДРАТНА_ДУГА {
      перший_байт[0] = відповідні_символи_юнікоду::ВІДКРИТА_КВАДРАТНА_ДУГА;
      вернути 1;
    }
    якщо символ_КД == символи::ЗАКРИТА_КВАДРАТНА_ДУГА {
      перший_байт[0] = відповідні_символи_юнікоду::ЗАКРИТА_КВАДРАТНА_ДУГА;
      вернути 1;
